<?xml version="1.0" encoding="utf-8"?>
<FrameLayout
  xmlns:android="http://schemas.android.com/apk/res/android"
  android:layout_width="match_parent"
  android:layout_height="match_parent">
    <RelativeLayout android:id="@+id/relativeLayout1" android:layout_gravity="center" android:layout_height="wrap_content" android:layout_width="wrap_content">
        <ImageView android:id="@+id/imageView1" android:layout_height="wrap_content" android:layout_width="wrap_content" android:layout_alignParentLeft="true" android:background="@drawable/circle" android:src="@drawable/circle"></ImageView>
        <ImageView android:id="@+id/imageView4" android:layout_height="wrap_content" android:layout_width="wrap_content" android:layout_below="@+id/imageView1" android:layout_alignLeft="@+id/imageView1" android:layout_alignRight="@+id/imageView1" android:src="@drawable/circle"></ImageView>
        <ImageView android:id="@+id/imageView2" android:layout_height="wrap_content" android:layout_width="wrap_content" android:layout_toRightOf="@+id/imageView1" android:layout_alignTop="@+id/imageView1" android:layout_alignBottom="@+id/imageView1" android:src="@drawable/circle"></ImageView>
        <ImageView android:id="@+id/imageView3" android:layout_height="wrap_content" android:layout_width="wrap_content" android:layout_toRightOf="@+id/imageView2" android:layout_alignTop="@+id/imageView2" android:layout_alignBottom="@+id/imageView2" android:src="@drawable/circle"></ImageView>
    </RelativeLayout>
    <Button android:layout_height="wrap_content" android:layout_width="wrap_content" android:text="Button" android:id="@+id/game_to_menu"></Button>
</FrameLayout>
